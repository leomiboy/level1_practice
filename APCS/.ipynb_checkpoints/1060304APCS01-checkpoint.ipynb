   "source": [
    "data = list(map(int,list(input())))\n",
    "odd = 0\n",
    "even = 0\n",
    "for i in range(len(data)):\n",
    "    if i % 2 == 0:\n",
    "        odd = odd + data[i]\n",
    "    else:\n",
    "        even = even + data[i]\n",
    "\n",
    "ans = odd - even\n",
    "if ans < 0:\n",
    "    ans = (-1)*ans\n",
    "print(ans)"
   ]
