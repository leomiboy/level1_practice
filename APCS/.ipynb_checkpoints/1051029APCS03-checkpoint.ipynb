   "source": [
    "line = list(map(int,input().split()))\n",
    "man = line[0]\n",
    "num = line[1]\n",
    "time = line[2]\n",
    "L =list(range(1,man+1))\n",
    "bomb = 0\n",
    "while bomb < time:\n",
    "    ans = L[num:]+L[:num-1]\n",
    "    bomb = bomb + 1\n",
    "print(ans[0])"
